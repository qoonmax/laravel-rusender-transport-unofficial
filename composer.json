{
  "name": "qoonmax/laravel-rusender-transport-unofficial",
  "description": "RuSender API integration (unofficial) for email dispatch in Laravel applications.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "qoonmax",
      "email": "qoonmax@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Qoonmax\\RuSenderApiMailTransport\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qoonmax\\RuSenderApiMailTransport\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Qoonmax\\RuSenderApiMailTransport\\RuSenderTransportProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "version": "0.1.0"
}
